{
  "version": 2,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "upgrade-insecure-requests"
        }
      ]
    }
  ],
  "rewrites": [
    { "source": "/api/(.*)", "destination": "http://3.26.165.228:8080/api/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}